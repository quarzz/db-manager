<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  
  <servlet>
    <servlet-name>TableController</servlet-name>
    <servlet-class>main.controller.TableController</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>ApplicationController</servlet-name>
    <servlet-class>main.controller.ApplicationController</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>QueryController</servlet-name>
    <servlet-class>main.controller.QueryController</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>TriggerController</servlet-name>
    <servlet-class>main.controller.TriggerController</servlet-class>
  </servlet>

  <servlet-mapping>
    <servlet-name>TableController</servlet-name>
    <url-pattern>/table</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
      <servlet-name>ApplicationController</servlet-name>
      <url-pattern></url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>QueryController</servlet-name>
    <url-pattern>/query</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>TriggerController</servlet-name>
    <url-pattern>/trigger</url-pattern>
  </servlet-mapping>
</web-app>
